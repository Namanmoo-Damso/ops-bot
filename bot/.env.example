# Bot client configuration
OPS_API_URL=https://n.sodam.store

# Optional / 안 넣어도 됨
ADMIN_ACCESS_TOKEN=<your-admin-token>

# AWS Credentials (for Transcribe, Polly)
AWS_ACCESS_KEY_ID=<your-aws-access-key>
AWS_SECRET_ACCESS_KEY=<your-aws-secret-key>
AWS_DEFAULT_REGION=us-east-1

# Optional
LOG_LEVEL=INFO

# User ID (DB의 user 테이블에서 admin이 아닌 유저 ID)
# httpx.HTTPStatusError: Client error '404 Not Found' 발생시 USER_ID 확인
USER_ID=<your-user-id>
